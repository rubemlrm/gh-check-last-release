        "@actions/github": "^4.0.0",
        "@octokit/rest": "^18.0.9"
    },
    "devDependencies": {
        "@types/jest": "^27.0.2",
        "@types/node": "^16.11.25",
        "@types/semver": "^6.0.0",
        "@typescript-eslint/eslint-plugin": "^5.16.0",
        "@vercel/ncc": "^0.33.3",
        "jest": "^27.2.5",
        "jest-circus": "^27.2.5",
        "nock": "^10.0.6",
        "prettier": "^1.17.1",
        "ts-jest": "^27.0.5",
        "typescript": "^3.8.3"
    }
}
